#include<stdio.h>
int main()
{
 int a,b,c;
 printf("Enter the angles of triangle(in degree):  \n");
 scanf("%d %d %d",&a,&b,&c);
 if(a+b+c!=180) {
 	printf("Triangle doesn't exist");
}
 else if(a==b && b==c)
   {
     printf("Equilateral Triangle\n");
}
 else if(a==b||b==c||c==a) {
    printf("Isocelese\n");   }
 else {
    printf("Scalene\n"); }   
 return 0; 
}
