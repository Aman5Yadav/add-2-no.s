#include<stdio.h>																																																																												
#include<math.h>
int main()
{
	int c,*p,*q,a,b;
	printf(" Enter 2 no.s ");
	scanf("%d %d",&a,&b);
	p = &a;
	q= &b;
	c= *p+*q;
	printf("sum of the 2 no.s = %d ",c);
	return 0;
}
*// OUTPUT
 Enter 2 no.s : 10 20
sum of the 2 no.s = 30
